{
  "name": "rocco-api",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "npm run clean && prisma generate && tsc",
    "clean": "rm -rf build",
    "dev": "npm run build && nodemon --config nodemon.json",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:dev": "npx prisma migrate dev",
    "prisma:generate": "npx prisma generate",
    "prisma:push": "npx prisma db push",
    "prisma:reset": "npx prisma migrate reset",
    "start": "node build/src/index",
    "test": "NODE_ENV=test jest",
    "typecheck": "tsc --noEmit --incremental false"
  },
  "dependencies": {
    "@fastify/circuit-breaker": "3.2.0",
    "@fastify/cookie": "^8.3.0",
    "@fastify/cors": "^8.3.0",
    "@fastify/csrf-protection": "^6.3.0",
    "@fastify/helmet": "^11.0.0",
    "@fastify/jwt": "^7.2.0",
    "@fastify/rate-limit": "^8.0.3",
    "@fastify/secure-session": "^6.1.0",
    "@prisma/client": "^5.0.0",
    "@segment/analytics-node": "^1.0.0",
    "@sendgrid/mail": "^7.6.1",
    "@sentry/node": "^7.60.1",
    "@sentry/tracing": "^7.60.0",
    "@total-typescript/ts-reset": "^0.4.2",
    "date-fns": "^2.28.0",
    "dotenv": "^16.3.1",
    "fastify": "^4.21.0",
    "fastify-plugin": "^4.5.1",
    "open-graph-scraper": "^6.2.2",
    "pino-pretty": "^10.2.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/jest": "29.5.2",
    "@types/node": "^20.4.5",
    "eslint-config-ponti": "0.0.0",
    "jest": "29.5.0",
    "nodemon": "^3.0.1",
    "prisma": "5.7.0",
    "ts-jest": "29.1.0",
    "ts-node": "10.4.0",
    "typescript": "^5.1.6"
  }
}

{
  "name": "aidea",
  "version": "1.4.0",
  "private": true,
  "scripts": {
    "cy": "cypress open",
    "cy:headless": "cypress run",
    "dev": "next dev -H 127.0.0.1 -p 5000",
    "build": "next build",
    "lint": "biome lint ./src",
    "lint:fix": "biome format ./src",
    "postinstall": "cypress install",
    "preview": "npm run build && next start --port 4173",
    "preview:test": "start-server-and-test preview http://localhost:4173",
    "start": "next start",
    "test": "vitest --run",
    "test:ci": "vitest run",
    "test:coverage": "pnpx vite preview --outDir coverage/lcov-report",
    "test:e2e": "npm run preview:test 'cypress open'",
    "test:e2e:ci": "npm run preview:test 'npx cypress run --record'",
    "test:e2e:headless": "npm run preview:test 'cypress run'",
    "test:update": "vitest run -u",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit --incremental false"
  },
  "dependencies": {
    "@emotion/react": "^11.0.0-rc.0",
    "@emotion/styled": "^11.11.0",
    "@hominem/db": "0.0.0",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@next/bundle-analyzer": "13.4.9",
    "@radix-ui/colors": "^1.0.0",
    "@radix-ui/react-avatar": "^1.0.3",
    "@radix-ui/react-dropdown-menu": "^2.0.5",
    "@radix-ui/react-icons": "^1.3.0",
    "@tanstack/react-query": "4.29.19",
    "@trpc/client": "10.34.0",
    "@trpc/next": "10.34.0",
    "@trpc/react-query": "10.34.0",
    "@trpc/server": "10.34.0",
    "cheerio": "1.0.0-rc.12",
    "classnames": "^2.3.2",
    "daisyui": "^3.2.1",
    "ky": "^0.33.3",
    "lucide-react": "^0.363.0",
    "next": "^14.1.3",
    "next-auth": "^4.24.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-query": "^3.39.2",
    "superjson": "1.12.4",
    "ui": "0.0.0",
    "zod": "^3.22.3"
  },
  "devDependencies": {
    "@testing-library/cypress": "10.0.1",
    "@testing-library/react": "14.0.0",
    "@types/node": "20.4.1",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.23",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "1.4.0",
    "autoprefixer": "^10.4.12",
    "cypress": "^13.7.1",
    "dotenv-cli": "^7.3.0",
    "husky": "^8.0.1",
    "jsdom": "^22.1.0",
    "next-router-mock": "^0.9.7",
    "postcss": "8.4.31",
    "postcss-import": "^15.1.0",
    "prettier-plugin-tailwindcss": "^0.4.0",
    "rollup-plugin-analyzer": "^4.0.0",
    "start-server-and-test": "^2.0.0",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier": "^9.0.3",
    "tailwindcss": "3.3.2",
    "typescript": "5.4.3",
    "vite": "^5.2.6",
    "vitest": "^1.4.0"
  },
  "nyc": {
    "report-dir": "coverage/cypress",
    "reporter": [
      "lcov"
    ]
  },
  "engines": {
    "node": ">=20.0.0"
  }
}

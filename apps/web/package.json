{
  "name": "rocco-web",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "build:analyze": "tsc && ANALYZE=true vite build",
    "dev": "(npx spotlight-sidecar) & vite",
    "lint:fix": "biome format --write ./src",
    "lint:stylelint": "stylelint --config .stylelintrc.json --cache --fix 'src/**/*.{css,scss}'",
    "lint": "biome lint ./src && npm run lint:stylelint",
    "postinstall": "cypress install",
    "preview:test": "start-server-and-test preview http://localhost:53422",
    "preview": "vite preview",
    "start": "npx http-server build --gzip -a 0.0.0.0 -p $PORT -c 1209600",
    "test": "NODE_ENV=test vitest --run",
    "test:coverage": "vitest --run --coverage",
    "test:coverage:view": "npx vite preview --outDir coverage/lcov-report",
    "test:e2e": "npm run preview:test 'npx cypress open'",
    "test:e2e:ci": "npm run preview:test 'npx cypress run --record false'",
    "test:e2e:headless": "npm run preview:test 'npx cypress run'",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit --incremental false"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@headlessui/react": "^1.7.15",
    "@hominem/components": "0.0.0",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@reduxjs/toolkit": "^1.7.2",
    "@tanstack/react-query": "^5.35.1",
    "@vis.gl/react-google-maps": "^1.0.0",
    "axios": "^1.6.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "daisyui": "^4.10.5",
    "formik": "^2.2.9",
    "lucide-react": "^0.320.0",
    "prop-types": "^15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.23.0",
    "tailwindcss-animate": "^1.0.7",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@playwright/test": "^1.44.0",
    "@spotlightjs/spotlight": "^1.2.5",
    "@tanstack/react-query-devtools": "^5.35.1",
    "@testing-library/cypress": "10.0.1",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20.4.5",
    "@types/react": "^18.2.23",
    "@types/react-dom": "^18.2.23",
    "@types/react-redux": "^7.1.22",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "1.4.0",
    "autoprefixer": "^10.4.13",
    "cypress": "^13.8.1",
    "dotenv": "^16.4.5",
    "husky": "^8.0.1",
    "jsdom": "^22.1.0",
    "msw": "^2.2.13",
    "postcss": "8.4.31",
    "postcss-import": "^15.1.0",
    "react-test-renderer": "^18.2.0",
    "rollup-plugin-analyzer": "^4.0.0",
    "sass": "^1.49.10",
    "start-server-and-test": "^2.0.0",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier": "^9.0.3",
    "tailwind-merge": "^2.3.0",
    "tailwindcss": "^3.4.3",
    "tsconfig": "0.0.0",
    "typescript": "5.4.3",
    "vite": "^5.2.7",
    "vite-plugin-pwa": "^0.20.0",
    "vite-tsconfig-paths": "^4.2.3",
    "vitest": "^1.4.0"
  },
  "nyc": {
    "report-dir": "coverage/cypress",
    "reporter": [
      "lcov"
    ]
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.13.0"
  }
}

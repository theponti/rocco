{
  "name": "@hominem/web",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc && nx vite:build",
    "build:analyze": "tsc && ANALYZE=true nx vite:build",
    "dev": "(npx spotlight-sidecar) & vite",
    "lint:fix": "biome format --write ./src",
    "lint:stylelint": "stylelint --config .stylelintrc.json --cache --fix 'src/**/*.{css,scss}'",
    "lint": "biome lint ./src && npm run lint:stylelint",
    "preview:test": "start-server-and-test preview http://localhost:53422",
    "preview": "nx vite:preview",
    "start": "npx http-server build --gzip -a 0.0.0.0 -p $PORT -c 1209600",
    "test": "NODE_ENV=test nx vite:test --run",
    "test:coverage": "nx vite:test --run --coverage",
    "test:coverage:view": "npx nx vite:preview --outDir coverage/lcov-report",
    "test:e2e": "npm run preview:test 'npx cypress open'",
    "test:e2e:ci": "npm run preview:test 'npx cypress run --record false'",
    "test:e2e:headless": "npm run preview:test 'npx cypress run'",
    "test:watch": "nx vite:test",
    "typecheck": "tsc --noEmit --incremental false"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@hominem/utils": "*",
    "@headlessui/react": "^2.1.10",
    "@hominem/components": "0.0.0",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-popover": "^1.1.2",
    "@radix-ui/react-slot": "^1.1.0",
    "@reduxjs/toolkit": "^2.2.8",
    "@tanstack/react-query": "^5.59.9",
    "@vis.gl/react-google-maps": "^1.3.0",
    "axios": "^1.7.7",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "daisyui": "^4.12.13",
    "formik": "^2.4.6",
    "lucide-react": "^0.451.0",
    "prop-types": "^15.8.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.26.2",
    "tailwindcss-animate": "^1.0.7",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.8",
    "@playwright/test": "^1.48.0",
    "@spotlightjs/spotlight": "^2.5.0",
    "@tanstack/react-query-devtools": "^5.59.9",
    "@testing-library/cypress": "10.0.2",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^22.7.5",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "@types/react-redux": "^7.1.34",
    "@vitejs/plugin-react": "^4.3.2",
    "@vitest/coverage-v8": "2.1.2",
    "autoprefixer": "^10.4.20",
    "cypress": "^13.15.0",
    "dotenv": "^16.4.5",
    "husky": "^9.1.6",
    "jsdom": "^25.0.1",
    "msw": "^2.4.9",
    "postcss": "8.4.47",
    "postcss-import": "^16.1.0",
    "react-test-renderer": "^18.3.1",
    "rollup-plugin-analyzer": "^4.0.0",
    "sass": "^1.79.4",
    "start-server-and-test": "^2.0.8",
    "stylelint": "^16.9.0",
    "stylelint-config-prettier": "^9.0.5",
    "tailwind-merge": "^2.5.3",
    "tailwindcss": "^3.4.13",
    "tsconfig": "0.0.0",
    "typescript": "5.6.3",
    "vite": "^5.4.8",
    "vite-plugin-pwa": "^0.20.5",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.1.2"
  },
  "nyc": {
    "report-dir": "coverage/cypress",
    "reporter": [
      "lcov"
    ]
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.24.0"
  }
}

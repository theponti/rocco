{
	"name": "@pontistudios/rocco",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "react-router build",
		"build:analyze": "ANALYZE=true react-router build",
		"dev": "(npx spotlight-sidecar) & react-router dev",
		"format": "biome format --write ./app",
		"lint": "biome lint ./app",
		"preview:test": "start-server-and-test preview http://localhost:3000",
		"preview": "react-router build && react-router-serve ./build/server/index.js",
		"start": "react-router-serve ./build/server/index.js",
		"test": "NODE_ENV=test vitest run",
		"test:coverage": "vitest run --coverage",
		"test:coverage:view": "npx vite preview --outDir coverage/lcov-report",
		"test:e2e": "npm run preview:test 'npx cypress open'",
		"test:e2e:ci": "npm run preview:test 'npx cypress run --record false'",
		"test:e2e:headless": "npm run preview:test 'npx cypress run'",
		"test:watch": "vitest",
		"typecheck": "react-router typegen && tsc --noEmit --incremental false"
	},
	"dependencies": {
		"@clerk/react-router": "^1.3.4",
		"@emotion/react": "^11.13.3",
		"@emotion/styled": "^11.13.0",
		"@headlessui/react": "^2.1.10",
		"@radix-ui/react-dialog": "^1.1.2",
		"@radix-ui/react-dropdown-menu": "^2.1.2",
		"@radix-ui/react-icons": "^1.3.0",
		"@radix-ui/react-popover": "^1.1.2",
		"@radix-ui/react-slot": "^1.1.0",
		"@react-router/node": "^7.5.3",
		"@react-router/serve": "^7.5.3",
		"@reduxjs/toolkit": "^2.2.8",
		"@tanstack/react-query": "^5.59.9",
		"@vis.gl/react-google-maps": "^1.5.2",
		"@vitejs/plugin-react": "^4.4.1",
		"axios": "^1.7.7",
		"class-variance-authority": "^0.7.0",
		"clsx": "^2.1.1",
		"cmdk": "^1.0.0",
		"formik": "^2.4.6",
		"framer-motion": "^12.9.4",
		"isbot": "^5.1.27",
		"lucide-react": "^0.507.0",
		"prop-types": "^15.8.1",
		"react": "^19.1.0",
		"react-dom": "^19.1.0",
		"react-redux": "^9.1.2",
		"react-router": "^7.5.3",
		"yup": "^1.4.0"
	},
	"devDependencies": {
		"@babel/core": "^7.25.8",
		"@playwright/test": "^1.48.0",
		"@react-router/dev": "^7.5.3",
		"@spotlightjs/spotlight": "^2.5.0",
		"@tailwindcss/vite": "^4.1.4",
		"@tanstack/react-query-devtools": "^5.59.9",
		"@testing-library/cypress": "10.0.2",
		"@testing-library/jest-dom": "^6.5.0",
		"@testing-library/react": "16.0.1",
		"@testing-library/user-event": "^14.5.2",
		"@types/node": "^22.7.5",
		"@types/react": "^19.1.2",
		"@types/react-dom": "^19.1.2",
		"@types/react-redux": "^7.1.34",
		"@vitejs/plugin-react": "^4.4.1",
		"@vitest/coverage-v8": "^3.1.2",
		"autoprefixer": "^10.4.20",
		"cypress": "^13.15.0",
		"daisyui": "^5.0.35",
		"dotenv": "^16.4.5",
		"jsdom": "^25.0.1",
		"msw": "^2.4.9",
		"react-test-renderer": "^18.3.1",
		"rollup-plugin-analyzer": "^4.0.0",
		"rollup-plugin-visualizer": "^5.14.0",
		"sass": "^1.79.4",
		"start-server-and-test": "^2.0.8",
		"tailwindcss": "^4.1.4",
		"tailwind-merge": "3.2.0",
		"tailwindcss-animate": "^1.0.7",
		"typescript": "5.6.3",
		"vite": "6.3.3",
		"vite-tsconfig-paths": "^5.1.4",
		"vitest": "^3.1.2"
	},
	"nyc": {
		"report-dir": "coverage/cypress",
		"reporter": ["lcov"]
	},
	"optionalDependencies": {
		"@rollup/rollup-linux-x64-gnu": "^4.24.0"
	}
}
